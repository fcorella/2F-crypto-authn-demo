[Unit]
Description=demo: Two-Factor Cryptographic Authentication with a Fusion Credential

[Service]
ExecStart=/home/ec2-user/2F-crypto-authn-demo/demo.mjs
Restart=always
User=ec2-user
Group=ec2-user
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/home/ec2-user/2F-crypto-authn-demo

[Install]
WantedBy=multi-user.target